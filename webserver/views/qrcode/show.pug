extends ../layout

block content
  div
    h1= title
    div
      if qrcode
        div#qrcode_list
          <qrcode-image qrcodedata=#{qrcode.base64img}></qrcode-image>
        p= qrcode.title
      else
        p= "データがありません。"
    div#mylist_button 
      <mylist-button apitoken=#{apiToken} qrcodeuuid=#{qrcodeUuid}></mylist-button>
  
